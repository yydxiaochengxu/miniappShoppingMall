/* pages/record/record.wxss */
page{background:#f1f1f1;}
.listtile{
    padding:0 28rpx;height:76rpx;line-height:76rpx;font-size:34rpx;color:#353535;font-weight:bold;
}
.listarr{
    padding:10rpx 20rpx 20rpx 50rpx;
}
.list{
    width:100%;height:300rpx;border-radius:12rpx;box-sizing: border-box;margin-top:40rpx;padding:30rpx;position:relative;
}
.list::before{
    content:'';position:absolute;top:0;left:0;width:inherit;height:inherit;background:#ffffff;filter:blur(5rpx);z-index:-2;
}
.jian{
    position:absolute;top:0;right:30rpx;background:orangered;width:40rpx;height:72rpx;font-size:22rpx;text-align:center;line-height:40rpx;overflow: hidden;color:#ffffff;
}
.jian::after{
    content:'';position:absolute;bottom:-20rpx;left:0;width:40rpx;height:40rpx;background:#ffffff;transform:rotateZ(45deg)
}
.list_l{
    width:240rpx;height:240rpx;position:relative;
}
.listimg{
    width:260rpx;height:250rpx;position:absolute;top:-50rpx;left:-50rpx;box-shadow:6rpx 8rpx 20rpx 14rpx lightgray;border:solid 2rpx #ffffff;
}
.producttitle{
    font-size:32rpx;color:#353535;font-weight:bold;line-height:1;
}
.starfen{margin-top:15rpx;}
.starbox{
    width:125rpx;height:25rpx;overflow:hidden;background:url('data:image/png;base64,');background-size:25rpx;background-repeat: repeat-x;
}
.star{
    width:125rpx;height:25rpx;background:url('data:image/png;base64,');background-size:25rpx;background-repeat: repeat-x;
}
.fen{
    margin-left:24rpx;color:#79A4BB;font-size:22rpx;letter-spacing:2rpx;
}
.content{
    font-size:26rpx;color:#888888;word-break:break-all;margin-top:20rpx;
}

.moneybox{color:#79A4BB;font-size:26rpx;font-weight:bold;}
.money{font-size:30rpx;}
.point{font-size:22rpx;}